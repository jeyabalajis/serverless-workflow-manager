{
  "development": {
    "app_function": "workflow_event_handler.process_messages",
    "apigateway_enabled": false,
    "aws_region": "ap-south-1",
    "profile_name": "sandbox",
    "project_name": "workflowhandler",
    "runtime": "python3.6",
    "s3_bucket": "zappa-workflow-manager-dev",
    "environment_variables": {
      "env": "dev"
    },
    "events": [
      {
        "function": "workflow_event_handler.process_messages",
        "event_source": {
          "arn": "tbd",
          "batch_size": 1,
          "enabled": true
        }
      }
    ],
    "extra_permissions": [
      {
        "Effect": "Allow",
        "Action": [
          "secretsmanager:*"
        ],
        "Resource": "*"
      }
    ]
  },
  "production": {
    "apigateway_enabled": false,
    "aws_region": "ap-south-1",
    "profile_name": "sandbox",
    "project_name": "workflowhandler",
    "runtime": "python3.6",
    "s3_bucket": "zappa-workflow-manager-prod",
    "environment_variables": {
      "env": "prod"
    },
    "events": [
      {
        "function": "workflow_event_handler.process_messages",
        "event_source": {
          "arn": "tbd",
          "batch_size": 10,
          "enabled": true
        }
      }
    ],
    "extra_permissions": [
      {
        "Effect": "Allow",
        "Action": [
          "secretsmanager:*"
        ],
        "Resource": "*"
      }
    ],
    "timeout_seconds": 900
  }
}